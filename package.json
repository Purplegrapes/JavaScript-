{
  "name": "javascript-deep",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dll": "webpack -p --config webpack/webpack.dll.config.js",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod"
  },
  "betterScripts": {
    "build:dev": {
      "command": "babel-node  --max_old_space_size=6144 ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack/webpack.config.js --colors",
      "env": {
        "mode": "development"
      }
    },
    "build:prod": {
      "command": "webpack -p --config webpack/webpack.config.js --display-error-details --progress --colors",
      "env": {
        "mode": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Purplegrapes/JavaScript-deep.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Purplegrapes/JavaScript-deep/issues"
  },
  "homepage": "https://github.com/Purplegrapes/JavaScript-deep#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "clean-webpack-plugin": "0.1.17"
  }
}
